<header>
  <h1>Test form for new task?</h1>
</header>
<main>

  <%= form_for @new_task do |f| %>
    <fieldset>
      <%= f.label :name  %>
      <%= f.text_field :name %>

      <%= f.label :description  %>
      <%= f.text_area :description %>

      <%= f.label :completed_date  %>
      <%= f.text_field :completed_date %>
    </fieldset>
    <%= f.submit 'Submit Task' %>
  <% end %>
</main>
