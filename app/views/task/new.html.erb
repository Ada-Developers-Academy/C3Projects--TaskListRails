<h1> This is a new task form!</h1>

<%= form_for @task do |f| %>
  <fieldset>
    <legend>New Task Deets</legend>
    <%= f.label :taskname %>
    <%= f.text_field :taskname %>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </fieldset>

  <%= f.submit 'Create task' %>

<% end %>
